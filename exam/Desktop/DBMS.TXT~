SELECT * FROM BRANCH;
SELECT * FROM ACCOUNT;
SELECT * FROM DEPOSITOR;

SELECT CNAME FROM DEPOSITOR,ACCOUNT WHERE ACCOUNT.ACC_NO=DEPOSITOR.ACC_NO AND BR_NAME='MUMBAI'; 

SELECT ACC_NO FROM DEPOSITOR WHERE CNAME LIKE 'A%';

SELECT CNAME FROM DEPOSITOR AS D,ACCOUNT AS A WHERE D.ACC_NO=A.ACC_NO AND BALANCE=(SELECT MAX(BALANCE) FROM ACCOUNT);

SELECT CNAME FROM DEPOSITOR WHERE DATE('2016-04-12')-DOB>DATE('0065-0-0');


